# Menambah anggota set
print("# Menambah anggota set")
set_04 = {2, 3, 4, 5, 6}
print(set_04)
set_04.add(1)
print(set_04, "\n")

# Menghapus nilai 4
print("# Menghapus nilai 4")
set_04.discard(4)
print(set_04, "\n")

# Menambahkan nilai 10
print("# Menambahkan nilai 10")
set_04.add(10)
print(set_04)
